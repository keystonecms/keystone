{
  "name": "keystonecms/keystone",
  "description": "Keystone CMS â€“ a modern PHP content management system with online installer and updater. A stable, developer-first CMS core.",
  "license": "proprietary",
  "type": "project",
  "version": "v1.1.0",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "Keystone\\": "app/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Keystone\\Tests\\": "tests/"
    }
},
  "require": {
    "php": "^8.2",
    "slim/slim": "^4.12",
    "slim/twig-view": "^3.4",
    "slim/psr7": "^1.8",
    "vlucas/phpdotenv": "^5.6",
    "php-di/php-di": "^7.1",
    "monolog/monolog": "^3.10",
    "phpmailer/phpmailer": "^7.0",
    "pragmarx/google2fa": "^9.0",
    "bacon/bacon-qr-code": "^3.0",
    "symfony/var-dumper": "^7.4",
    "ramsey/uuid": "^4.9",
    "symfony/console": "^7.4",
    "mollie/mollie-api-php": "^3.8",
    "stripe/stripe-php": "^19.2",
    "guzzlehttp/guzzle": "^7.10",
    "symfony/process": "^7.4",
    "keystonecms/plugin-blog": "@dev",
    "keystonecms/plugin-pages": "@dev",
    "keystonecms/plugin-auth": "@dev",
    "keystonecms/plugin-shoppingcart": "dev-main"
  },
  "require-dev": {
    "phpunit/phpunit": "^12.5"
  },
  "scripts": {
    "test": "phpunit",
    "test:list": "phpunit --list-tests",
    "test:debug": "phpunit --debug",
    "post-version-bump": [
      "php tools/write-version.php"
       ]
    },
  "repositories": [
    {
      "type": "path",
      "url": "../keystone-plugins/*",
      "options": {
        "symlink": true
      }
    }
  ],
  "config": {
    "platform": {
      "php": "8.3.0"
    }
  }
}
